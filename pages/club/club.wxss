/* enterprise/clubList/clubList.wxss */
.clear{
  clear:both;
}
.club_info{
  width:100%;
}

.club_info_list{
  /* height:190rpx; */
  padding:40rpx 0rpx;
  border-bottom:1rpx solid #e5e5e5;
}
.club_info_list .left{
  width:25%;
  float:left;
  margin-top:10rpx;
}
.left_bor{
  width:140rpx;
  height:140rpx;
  margin:0 auto;
}
.club_info_img{
  width:100%;
  float:right;
  /* height:140rpx; */
}
.club_info_list .center{
  width:60%;
  float:left;
  /* margin-left:2.5%; */
  font-family: SourceHanSansCN-Regular;
	font-size: 34rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #d6d6d6;
}
.club_name{
  color:#000000;
  font-size: 34rpx;
}
.right{
  float:right;
  width:10%;
}
.region{
  margin-top: 20rpx;
  font-size: 26rpx;
}
.works_num{
  color:#6ac4d2;
  margin-top: 20rpx;
}

