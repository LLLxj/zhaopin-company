.banner{
  width:100%;
}

.list{
  width: 100%;
	height: 235rpx;
	background-color: #ffffff;
  border-bottom: 10rpx solid #fafafa;
  position:relative;
}

.list .content{
  width:94%;
  padding:0 3%;
  height: 235rpx;
  margin:0 auto;
  /* background:#f00; */
   position:relative; 
  /* left: -100rpx; */
}

.list .content>view{
  width: 100%;
  height: 33.33%;
}

.list .content .top{
  /* background:#00f; */
  line-height: 120rpx; 
}

.list .content .centent{
  /* background:#000; */
	line-height: 85rpx;
}

.list .content .bottom{
  /* background:#ff0; */
	line-height: 37rpx;
  
}

.top .left,.centent .left{
  width:70%;
  height: 100%;
  float:left;
  font-family: SourceHanSansCN-Regular;
	font-size: 34rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #000000;

}

.top .right,.centent .right{
  width:30%;
  height: 100%;
  float:right;
  text-align: right;
  font-family: ArialMT;
	font-size: 34rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #6ac4d2;
  
}

.centent .left{
  font-family: SourceHanSansCN-Regular;
	font-size: 26rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #d6d6d6;
}

.centent .right{
  font-family: ArialMT;
	font-size: 34rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #e42387;
}

.bottom .logo{
  width:37rpx;
  height:37rpx;
  float:left;
}
.bottom .logo image{
width:37rpx;
}
.bottom .cname{
  width:92%;
  height: 100%;
  float:right;
  font-family: SourceHanSansCN-Regular;
	font-size: 26rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #d6d6d6;  
}

.line{
  width:100%;
  height: 88rpx;
  background:#6bc4d2;
  position:fixed;
  bottom: 0rpx;
  font-family: SourceHanSansCN-Regular;
	font-size: 36rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #ffffff;
  text-align: center;
  line-height: 88rpx;
}

.delete{
  /* position:relative; */
  width: 100%;
  height: 235rpx;
  margin:0 auto;
  background: #fff; 
  z-index: 9999;
  background-image: url(https://zhaopin.heigrace.com/public/static/xiaochengxu/icon/del.jpg);
  background-repeat:no-repeat;
  background-position:right;
  background-size: auto 100%;
}

